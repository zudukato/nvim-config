syntax on

set number
set mouse=a
set rnu
set encoding=utf-8
set showcmd
set expandtab
set nobackup
set ignorecase
set smartindent
set numberwidth=1
set incsearch
set cursorline
set noerrorbells
set clipboard=unnamedplus

so ~/.config/nvim/Pluggins.vim
so ~/.config/nvim/maps.vim

set background=dark
colorscheme seoul256
